{
  "manifest_version": 3,
  "name": "연습",
  "version": "1.0",
  "description": "설명입니당.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": "images/32.png",
    "default_title": "연습중",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/inject.js"]
    }
  ]
}
